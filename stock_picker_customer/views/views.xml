<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- product product -->
        <record id="stock_picking_resale" model="ir.ui.view">
            <field name="name">stock.picking.form.for_resale</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock_dermanord.view_picking_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='prio']" position="before">
                    <field name="is_reseller"/>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
